{
  "name": "jest-badges-action",
  "description": "Generates coverage badges and pushes them to the repo",
  "version": "1.8.5",
  "main": "lib/main.js",
  "repository": "https://github.com/jpb06/jest-badges-action.git",
  "author": "jpb06 <jpb.06@outlook.com>",
  "license": "MIT",
  "scripts": {
    "cleanDist": "rimraf ./dist && rimraf ./lib",
    "build": "yarn cleanDist && tsc --project tsconfig.prod.json && ncc build --minify",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:dev": "jest --watchAll --coverage",
    "test:ci": "jest --ci --coverage",
    "badges": "generateBadges"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "fs-extra": "^10.0.0",
    "node-jest-badges": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/prettier": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
